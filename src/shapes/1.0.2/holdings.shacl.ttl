@prefix rico:  <https://www.ica.org/standards/RiC/ontology#> .
@prefix ehri_institutions: <http://lod.ehri-project-test.eu/institutions/> .

ehri:MaterialScript  a  sh:NodeShape ;
        sh:nodeKind     sh:IRI ;
        sh:property     [ sh:datatype  xsd:string ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:name
                        ] ;
        sh:property     [ sh:in    ( ehri:MaterialScript ) ;
                          sh:path  rdf:type
                        ] ;
        sh:targetClass  ehri:MaterialScript .

ehri:HoldingParralelFormOfName
        a               sh:NodeShape ;
        sh:nodeKind     sh:IRI ;
        sh:property     [ sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:isOrWasTitleOf
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:name
                        ] ;
        sh:property     [ sh:in    ( rico:Title ) ;
                          sh:path  rdf:type
                        ] ;
        sh:targetClass  rico:Title .


ehri:Holding  a         sh:NodeShape ;
        sh:nodeKind     sh:IRI ;
        sh:property     [ sh:maxCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:hasRecordSetType
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:locationOfOriginals
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:otherFindingAids
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:scopeAndContent
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:sources
                        ] ;
        sh:property     [ sh:node  ehri:MaterialScript ;
                          sh:path  ehri:scriptOfMaterials
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:wasUsedAsSourceIn
                        ] ;
        sh:property     [ sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:hasRecordSetType
                        ] ;
        sh:property     [ sh:maxCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:isOrWasIncludedIn
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:archivistNote
                        ] ;
        sh:property     [ sh:node  ehri:HoldingBeginningDate ;
                          sh:path  rico:hasBeginningDate
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:recordResourceStructure
                        ] ;
        sh:property     [ sh:node  ehri:Acquisition ;
                          sh:path  rico:resultsOrResultedFrom ;
                          sh:maxCount  1 
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:appraisal
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:relatedMaterial
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:physicalCharacterisiticsAndTechnicalRequirements
                        ] ;
        sh:property     [ sh:node  ehri:HoldingEndDate ;
                          sh:path  rico:hasEndDate
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:conditionsOfUse
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:locationOfCopies
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:accruals
                        ] ;
        sh:property     [ sh:datatype  xsd:string ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:conditionsOfUse
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:recordResourceExtent
                        ] ;
        sh:property     [ sh:nodeKind  sh:IRI ;
                          sh:path      rdfs:seeAlso ;
                          sh:maxCount  1 ;
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:minCount  1 ;
                          sh:path      rico:title
                        ] ;
        sh:property     [ sh:datatype  xsd:string ;
                          sh:maxCount  2 ;
                          sh:path      rico:date
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:notes
                        ] ;
        sh:property     [ sh:datatype  xsd:string ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:date
                        ] ;
        sh:property     [ sh:node  ehri:AlternateIds ;
                          sh:path  rico:hasOrHadIdentifier
                        ] ;
        sh:property     [ sh:node  ehri:HoldingParralelFormOfName ;
                          sh:path  rico:hasOrHadTitle ;
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:history
                        ] ;
        sh:property     [ sh:nodeKind  sh:Literal ;
                          sh:maxCount  1 ;
                          sh:path      rico:identifier
                        ] ;
        sh:property     [ sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:hasOrHadHolder
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      ehri:separatedMaterial
                        ] ;
        sh:property     [ sh:datatype  rdf:langString ;
                          sh:path      rico:conditionsOfAccess
                        ] ;
        sh:property     [ sh:datatype  xsd:string ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:history
                        ] ;
        sh:property     [ sh:nodeKind  sh:IRI ;
                          sh:path      rico:includesOrIncluded
                        ] ;
        sh:property     [ sh:nodeKind  sh:IRI ;
                          sh:path      ehri:isCopyOf
                        ] ;                         
        sh:property     [ sh:nodeKind  sh:IRI ;
                          sh:path      ehri:hasCopy
                        ] ;      
        sh:property     [ sh:nodeKind  sh:IRI ;
                          sh:path      rico:hasCreator
                        ] ;                                              
        sh:targetClass  ehri:Holding ;
        sh:or (
          [ 
            sh:property [ 
                          sh:node  ehri:Language ;
                          sh:path  rico:hasOrHadLanguage
                        ] ;
            sh:property [ 
                          sh:in    ( ehri:Record ) ;
                          sh:path  rdf:type
                        ] ;
            sh:targetClass  ehri:Record 
          ]
          [ 
            sh:property [ 
                          sh:node  ehri:Language ;
                          sh:path  rico:hasOrHadSomeMembersWithLanguage
                        ] ;
            sh:property [ 
                          sh:in    ( ehri:RecordSet ) ;
                          sh:path  rdf:type
                        ] ;
            sh:targetClass  ehri:RecordSet 
          ]
        ) ;
        sh:nodeKind     sh:IRI .

ehri:HoldingEndDate  a  sh:NodeShape ;
        sh:nodeKind     sh:BlankNode ;
        sh:property     [ sh:minCount  1 ;
                          sh:nodeKind  sh:IRI ;
                          sh:path      rico:isEndDateOf
                        ] ;
        sh:property     [ sh:nodeKind  sh:Literal ;
                          sh:maxCount  1 ;
                          sh:minCount  1 ;
                          sh:path      rico:normalizedDateValue
                        ] ;
        sh:property     [ sh:in    ( rico:Date ) ;
                          sh:path  rdf:type
                        ] .

